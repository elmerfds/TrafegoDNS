{
  "name": "trafegodns",
  "version": "2.0.0",
  "description": "Automatic DNS management for Docker containers with support for multiple DNS providers",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "dns",
    "docker",
    "traefik",
    "cloudflare",
    "route53",
    "digitalocean",
    "technitium"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-route-53": "^3.787.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.7.0",
    "cloudflare": "^5.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dockerode": "^4.0.6",
    "drizzle-orm": "^0.38.4",
    "express": "^5.0.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/dockerode": "^3.3.34",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.12.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitest/coverage-v8": "^3.0.4",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.19.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
